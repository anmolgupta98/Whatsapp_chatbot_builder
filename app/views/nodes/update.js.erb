<% botresponses_box_after = "botresponses_box_after_" + (@node.id).to_s %>
<% botmessages_box_compressed_partial = "botmessages_box_compressed_partial_" + (@parent_id).to_s%>
<% botresponses_box_compressed = "botresponses_box_compressed_" + (@node.id).to_s%>
<% botmessages_previous_compressed = "botmessages_previous_compressed_" + (@node.id).to_s%>
<% usermessagemain_partial_with_connector = "usermessagemain_partial_with_connector_" + (@parent_id).to_s%>
<% botmessage_compressed_name = "botmessage_compressed_name_" + (@node.id).to_s%>

document.getElementById('<%= botmessages_box_compressed_partial%>').innerHTML = "<%= j render 'home/edit/nodes/botmessages_box_compressed', parent_id: @parent_id%>";
document.getElementById('<%= botresponses_box_after%>').classList.add('hide');
document.getElementById('<%= botresponses_box_compressed %>').classList.remove('hide');
document.getElementById('<%= botmessages_previous_compressed %>').innerHTML = "<%= j render 'home/edit/nodes/botmessages_previous', delete_button: false, parent_id: @parent_id%>";
document.getElementById('<%= botmessage_compressed_name%>').innerHTML = "<%= j render 'home/edit/nodes/botmessages_name_field', node: @node, edit_tag: false%>";

<% temperory_node_save = @node.id%>
console.log('<%= @node.id%>');
console.log('<%= temperory_node_save %>');

console.log('<%= temperory_node_save %>');

<% for child in Node.find(temperory_node_save).children%>
	<% compressed_define_id = "compressed_define_nodes_id_" + child.id.to_s %>
	<% define_id = "define_nodes_id_" + child.id.to_s %>
	<%if child.name != nil || child.user_input_type != nil%>
		document.getElementById('<%= compressed_define_id%>').classList.remove('hide');
		document.getElementById('<%= define_id%>').classList.add('hide');
	<%end%>
<%end%>

document.getElementById('<%= usermessagemain_partial_with_connector %>').innerHTML = "<%= j render 'home/edit/nodes/usermessagesmain_with_connector'%>";
