<% connector_black_link_vertical = "connector_black_link_vertical_" + (@node.id).to_s%>
<% usermessages_define_node_partial = "usermessages_define_node_partial_" + (@node.id).to_s%>

document.getElementById('<%= usermessages_define_node_partial %>').innerHTML = "<%= j render 'home/edit/nodes/usermessages_define_node_with_forloop'%>";
height = document.getElementById('<%= connector_black_link_vertical %>').style.height;
height = parseInt(height);

<% height = 53%>
<% for i in @node.children%>
<%if i.node_type == "user"%>
	<% if i.name == nil && i.user_input_type == nil%>
		<% height = height + 52%>
	<%else%>
		<%height = height + 108%>
	<%end %>	
<% end %>
<% end %>
<% height = height.to_s + "px"%>
height = "<%= height%>";
height = parseInt(height);

document.getElementById('<%= connector_black_link_vertical %>').style.height =  height + "px";

<% for child in @node.children%>
	<% compressed_define_id = "compressed_define_nodes_id_" + child.id.to_s %>
	<% define_id = "define_nodes_id_" + child.id.to_s %>
	<%if child.name != nil || child.user_input_type != nil%>
		document.getElementById('<%= compressed_define_id%>').classList.remove('hide');
		document.getElementById('<%= define_id%>').classList.add('hide');
	<%end%>
<%end%>


